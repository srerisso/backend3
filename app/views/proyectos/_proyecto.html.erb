<tbody class="divide-y divide-gray-100 border-t border-gray-100">
  <tr class="hover:bg-gray-50">
    <td class="px-6 py-4"><%= proyecto.Codigo_Alg %></td>
    <td class="px-6 py-4"><%= proyecto.Referencia %></td>
    <td class="px-6 py-4"><%= proyecto.cliente.Nombre %></td>
    <td class="px-6 py-4"><%= proyecto.published_at %></td>
    <td class="px-6 py-4"><%= proyecto.Estado %></td>

    <td>
        <% if action_name != "show" %>
          <%= link_to proyecto, class: "rounded-sm py-2 px-2 inline-block" do %>
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="w-6 h-6">
            <path d="M12 15a3 3 0 100-6 3 3 0 000 6z" />
            <path fill-rule="evenodd" d="M1.323 11.447C2.811 6.976 7.028 3.75 12.001 3.75c4.97 0 9.185 3.223 10.675 7.69.12.362.12.752 0 1.113-1.487 4.471-5.705 7.697-10.677 7.697-4.97 0-9.186-3.223-10.675-7.69a1.762 1.762 0 010-1.113zM17.25 12a5.25 5.25 0 11-10.5 0 5.25 5.25 0 0110.5 0z" clip-rule="evenodd" />
          </svg>
          <% end %>

          <%= link_to edit_proyecto_path(proyecto), class: "rounded-sm py-2 ml-2 px-2 inline-block font-medium" do %>
            <!-- Heroicon name: solid -->
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="w-6 h-6">
              <path d="M21.731 2.269a2.625 2.625 0 00-3.712 0l-1.157 1.157 3.712 3.712 1.157-1.157a2.625 2.625 0 000-3.712zM19.513 8.199l-3.712-3.712-8.4 8.4a5.25 5.25 0 00-1.32 2.214l-.8 2.685a.75.75 0 00.933.933l2.685-.8a5.25 5.25 0 002.214-1.32l8.4-8.4z" />
              <path d="M5.25 5.25a3 3 0 00-3 3v10.5a3 3 0 003 3h10.5a3 3 0 003-3V13.5a.75.75 0 00-1.5 0v5.25a1.5 1.5 0 01-1.5 1.5H5.25a1.5 1.5 0 01-1.5-1.5V8.25a1.5 1.5 0 011.5-1.5h5.25a.75.75 0 000-1.5H5.25z" />
            </svg>
          <% end %>
        <% end %>
    </td>

  </tr>
</tbody>
