<div id="<%= dom_id matrices_movimiento %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Matrix:</strong>
    <%= matrices_movimiento.matrix %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Published at:</strong>
    <%= matrices_movimiento.published_at %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Created by:</strong>
    <%= matrices_movimiento.created_by %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Updated by:</strong>
    <%= matrices_movimiento.updated_by %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Cliente:</strong>
    <%= matrices_movimiento.cliente %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Fechamovimiento:</strong>
    <%= matrices_movimiento.FechaMovimiento %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Comentarios:</strong>
    <%= matrices_movimiento.Comentarios %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Quien:</strong>
    <%= matrices_movimiento.Quien %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Movimiento:</strong>
    <%= matrices_movimiento.Movimiento %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Matriz:</strong>
    <%= matrices_movimiento.matriz %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this matrices movimiento", matrices_movimiento, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this matrices movimiento', edit_matrices_movimiento_path(matrices_movimiento), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
